{
  "name": "@repo/google-integration",
  "version": "0.1.0",
  "private": true,
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@repo/shared": "file:../packages/shared",
    "google-auth-library": "^9.4.1",
    "googleapis": "^129.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@repo/eslint-config": "file:../packages/eslint-config",
    "@repo/tsconfig": "file:../packages/tsconfig",
    "@types/nock": "^10.0.3",
    "@types/xlsx": "^0.0.36",
    "nock": "^14.0.8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.6.1"
  }
}
